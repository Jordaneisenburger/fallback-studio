    "redux-thunk": "~2.3.0",
    "rimraf": "~2.6.3",
    "style-loader": "~0.23.1",
    "terser-webpack-plugin": "~1.2.3",
    "uuid": "~3.3.2",
    "webpack": "~4.38.0",
    "webpack-bundle-analyzer": "~3.3.2",
    "webpack-cli": "~3.2.3",
    "webpack-dev-server": "~3.2.1",
    "workbox-webpack-plugin": "~4.2.0"
  },
  "optionalDependencies": {
    "iltorb": "~2.0.0",
    "node-zopfli-es": "~1.0.3",
    "sharp": "~0.22.1"
  },
  "engines": {
    "node": "10.x",
    "yarn": ">=1.12.0"
  },
  "module": "src/index.js",
  "es2015": "src/index.js",
  "esnext": "src/index.js"
}
